version 3.0;
acl	aclfile18;
authenticate (user) {
	method=SSL;
	database=LDAP;
};
allow (read, write, execute, create) dns="*.mcom.com";
authenticate (user) {
	method=basic;
	database=20;
};
allow (read, write, execute, create) (timeofday>1700 or timeofday<0800);
