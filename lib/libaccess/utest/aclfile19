version 3.0;
acl	aclfile19A;
deny (read, write, execute, create) dns!="*.mcom.com";
allow absolute (read) ((timeofday>1700 or timeofday<0800) or dayofweek=satsunmon);

acl aclfile19B;
deny (write) dns="*.mcom.com";
